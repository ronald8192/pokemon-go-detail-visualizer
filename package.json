{
  "name": "pokemon-go-detail-visualizer",
  "version": "0.0.1",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run sass:w\" \"npm run lite\" ",
    "lite": "lite-server",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "sass": "node-sass app/scss/ --output app/css/ --source-map true",
    "sass:w": "node-sass -w app/scss/ --output app/css/ --source-map true",
    "typings": "typings"
  },
  "license": "MIT",
  "author": "ronald8192",
  "repository": {
    "type": "git",
    "url": "https://github.com/ronald8192/pokemon-go-detail-visualizer.git"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/forms": "0.2.0",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/router": "3.0.0-beta.1",
    "@angular/router-deprecated": "2.0.0-rc.2",
    "@angular/upgrade": "2.0.0-rc.4",
    "systemjs": "0.19.27",
    "core-js": "^2.4.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "^0.6.12",
    "angular2-in-memory-web-api": "0.0.14",
    "bootstrap": "^3.3.6",
    "jquery": "^2.2.4"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "gulp": "~3.9.1",
    "gulp-clean-css": "~2.0.11",
    "gulp-minify": "0.0.12",
    "gulp-plumber": "^1.1.0",
    "gulp-sass": "~2.3.2",
    "gulp-sourcemaps": "~1.6.0",
    "gulp-typescript": "~2.13.6",
    "gulp-webserver": "~0.9.1",
    "lite-server": "^2.2.0",
    "node-sass": "^3.8.0",
    "run-sequence": "~1.2.2",
    "typescript": "^1.8.10",
    "typings": "^1.0.4"
  }
}
